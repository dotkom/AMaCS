{
  "name": "supreme-guacamole",
  "version": "1.0.0",
  "description": "Committee application application for Online online.",
  "main": "index.jsx",
  "scripts": {
    "start": "webpack-dev-server --inline --hot",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "webpack",
    "build:prod": "webpack --config webpack.config.production.js",
    "watch": "webpack -w",
    "dev": "webpack-dev-server --watch-poll --inline --hot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotkom/supreme-guacamole.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotkom/supreme-guacamole/issues"
  },
  "homepage": "https://github.com/dotkom/supreme-guacamole#readme",
  "browserslist": [
    "last 2 versions",
    "> 5% in NO"
  ],
  "jest": {
    "rootDir": "app",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.svg$": "<rootDir>/__mocks__/svgMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!**/node_modules/**"
    ]
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "lodash": "^4.17.4",
    "oidc-client": "^1.3.0",
    "postcss-loader": "^2.0.6",
    "raven-js": "^3.17.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^15.6.1",
    "react-textarea-autosize": "^5.1.0",
    "rxjs": "^5.4.3",
    "style-loader": "^0.18.2",
    "svg-react-loader": "^0.4.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.4",
    "webpack-merge": "^4.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "webpack-dev-server": "^2.7.1"
  }
}
