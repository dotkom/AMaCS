{
  "presets" : [
    ["env", {
      "modules": false
    }],
    "react"],
  "plugins": [
    "transform-runtime",
    "lodash",
    "react-hot-loader/babel",
    "transform-object-rest-spread",
    ["module-resolver", {
      "root": ["./app/src"]
    }]
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
